    <fragment
        android:id="@+id/inventorySendFragment"
        android:name="com.example.spe_logistic.ui.inventory.InventorySendFragment"
        android:label="fragment_inventory_send"
        tools:layout="@layout/fragment_inventory_send" />
    <fragment
        android:id="@+id/inventoryInventoryFragment"
        android:name="com.example.spe_logistic.ui.inventory.InventoryInventoryFragment"
        android:label="fragment_inventory_inventory"
