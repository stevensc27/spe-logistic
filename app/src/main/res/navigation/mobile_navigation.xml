<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_inventory">

    <fragment
        android:id="@+id/navigation_inventory"
        android:name="com.example.spe_logistic.ui.inventory.InventoryFragment"
        android:label="@string/title_inventory"
        tools:layout="@layout/fragment_inventory" />

    <fragment
        android:id="@+id/navigation_send"
        android:name="com.example.spe_logistic.ui.send.SendFragment"
        android:label="@string/title_send"
        tools:layout="@layout/fragment_send" />

    <fragment
        android:id="@+id/navigation_collect"
        android:name="com.example.spe_logistic.ui.collect.CollectFragment"
        android:label="@string/title_collect"
        tools:layout="@layout/fragment_collect" />

    <fragment
        android:id="@+id/navigation_references"
        android:name="com.example.spe_logistic.ui.references.ReferencesFragment"
        android:label="@string/title_references"
        tools:layout="@layout/fragment_references" />
    <fragment
        android:id="@+id/navigation_perfil"
        android:name="com.example.spe_logistic.ui.perfil.PerfilFragment"
        android:label="@string/title_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/collectItemFragment"
        android:name="com.example.spe_logistic.ui.collect.CollectItemFragment"
